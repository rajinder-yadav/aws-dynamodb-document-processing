{
  "name": "tsys-record-processing",
  "version": "1.0.0",
  "description": "TypeScript project for DynamoDB local development",
  "keywords": [
    "aws",
    "dynamodb",
    "typescript"
  ],
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "tsc",
    "dev": "pnpm processor-example",
    "record-example": "tsx src/record-example.ts",
    "processor-example": "tsx src/processor-example.ts",
    "processor-complete-example": "tsx src/processor-complete-example.ts",
    "create-tables": "tsx scripts/create-tables.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "lint": "oxlint --type-aware --type-check",
    "lint:fix": "oxlint --fix",
    "tsc": "tsc"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.991.0",
    "@aws-sdk/lib-dynamodb": "^3.991.0",
    "dotenv": "^17.3.1",
    "pino": "^10.3.1"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "oxfmt": "^0.33.0",
    "oxlint": "^1.48.0",
    "oxlint-tsgolint": "^0.14.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.29.3"
}
